a=int(input("escriba el primer numero "))
b=int(input("escriba el segundo numero "))
c=int(input("escriba el tercer numero "))
if a==b==c:
    print("los 3 numeros son iguales")
else:
    if a>b and a>c:
        print("el mayor es",a)
    if b>a and b>c:
        print("el mayor es",b)
    if c>a and c>b:
        print("el mayor es",c)
    if a<b and a<c:
        print("el menor es", a)
    if b<a and b<c:
        print("el menor es", b)
    if c<a and c<b:
        print("el menor es", c)
